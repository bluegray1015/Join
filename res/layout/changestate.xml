<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical"
    android:layout_width="fill_parent"
    android:layout_height="fill_parent"
    >

	<AbsoluteLayout
		android:layout_width="fill_parent"
		android:layout_height="fill_parent">
		
	
		<TextView 
 			android:id="@+id/changestateText"
 			android:text="@string/changestatelabel_name"
 			android:layout_width="wrap_content"
			android:layout_height="wrap_content"
			android:layout_y="15px" android:textSize="15sp" android:layout_x="10px"/>
			
		<EditText
			android:id="@+id/changestateEdit"
			android:text=""
			android:textSize="14sp"						android:singleLine="true"						android:maxLength="11"
			android:layout_height="wrap_content" android:layout_x="10px" android:layout_y="50px" android:layout_width="335px">
		</EditText>
		
		
		
	</AbsoluteLayout>
</LinearLayout>