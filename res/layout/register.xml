<?xml version="1.0" encoding="utf-8"?><ScrollView xmlns:android="http://schemas.android.com/apk/res/android"    android:layout_width="fill_parent"    android:layout_height="fill_parent"    android:background="@drawable/background"    android:orientation="vertical" >    <TableLayout        xmlns:android="http://schemas.android.com/apk/res/android"        android:layout_width="fill_parent"        android:layout_height="fill_parent"        android:orientation="vertical" >        <LinearLayout            android:layout_width="fill_parent"            android:layout_height="wrap_content"            android:layout_margin="05px"            android:orientation="horizontal" >            <TableLayout                android:layout_width="wrap_content"                android:layout_height="fill_parent"                android:layout_margin="05px"                android:orientation="vertical" >                <ImageView                    android:id="@+id/myImageView"                    android:layout_width="wrap_content"                    android:layout_height="wrap_content"                    android:src="@drawable/userelmo" />                <Button                    android:id="@+id/changephotoButton"                    android:layout_width="wrap_content"                    android:layout_height="wrap_content"                    android:text="@string/uploadphotobutton_name"                    android:textSize="12sp" />            </TableLayout>            <TableLayout                android:layout_width="fill_parent"                android:layout_height="fill_parent"                android:layout_margin="05px"                android:orientation="vertical" >                <TextView                    android:id="@+id/usernameText"                    android:layout_width="fill_parent"                    android:layout_height="wrap_content"                    android:text="@string/userlabel"                    android:textColor="#FF7F24"                    android:textSize="16sp" />                <com.join.LineEditText                    android:id="@+id/usernameEdit"                    android:layout_width="fill_parent"                    android:layout_height="wrap_content"                    android:background="@null"                    android:maxLength="11"                    android:singleLine="true"                    android:text=""                    android:textSize="14sp" />                <TextView                    android:id="@+id/passwordText"                    android:layout_width="fill_parent"                    android:layout_height="wrap_content"                    android:text="@string/passwordlabel"                    android:textColor="#FF7F24"                    android:textSize="16sp" />                <com.join.LineEditText                    android:id="@+id/passwordEdit"                    android:layout_width="fill_parent"                    android:layout_height="wrap_content"                    android:background="@null"                    android:maxLength="14"                    android:singleLine="true"                    android:text=""                    android:textSize="14sp" />                <TextView                    android:id="@+id/retypeText"                    android:layout_width="fill_parent"                    android:layout_height="wrap_content"                    android:text="@string/retypelabel"                    android:textColor="#FF7F24"                    android:textSize="16sp" />                <com.join.LineEditText                    android:id="@+id/retypeEdit"                    android:layout_width="fill_parent"                    android:layout_height="wrap_content"                    android:background="@null"                    android:maxLength="14"                    android:password="true"                    android:singleLine="true"                    android:text=""                    android:textSize="14sp" />                <TextView                    android:id="@+id/birthdayText"                    android:layout_width="fill_parent"                    android:layout_height="wrap_content"                    android:text="@string/birthdayadlabel"                    android:textColor="#FF7F24"                    android:textSize="16sp" />                <com.join.LineEditText                    android:id="@+id/birthdayEdit"                    android:layout_width="fill_parent"                    android:layout_height="wrap_content"                    android:background="@null"                    android:maxLength="14"                    android:singleLine="true"                    android:text=""                    android:textSize="14sp" />            </TableLayout>        </LinearLayout>        <RadioGroup            android:layout_width="fill_parent"            android:layout_height="wrap_content"            android:orientation="horizontal"            android:textColor="@color/red" >            <RadioButton                android:id="@+id/maleRadio"                android:layout_width="wrap_content"                android:layout_height="wrap_content"                android:text="@string/maleratiobutton_name"                android:textColor="@color/blue" />            <RadioButton                android:id="@+id/FemaleRadio"                android:layout_width="wrap_content"                android:layout_height="wrap_content"                android:text="@string/femaleratiobutton_name"                android:textColor="@color/red" />        </RadioGroup>        <LinearLayout            android:layout_width="fill_parent"            android:layout_height="wrap_content"            android:layout_margin="05px"            android:orientation="vertical" >            <TableLayout                android:layout_width="fill_parent"                android:layout_height="wrap_content"                android:orientation="vertical"                android:stretchColumns="1,3,5" >                <TableRow>                    <TextView                        android:id="@+id/countryText"                        android:layout_width="fill_parent"                        android:layout_height="wrap_content"                        android:text="@string/locationcountrylabel"                        android:textColor="#FF7F24"                        android:textSize="16sp" />                    <com.join.LineAutoCompleteTextView                        android:id="@+id/countryEdit"                        android:layout_width="fill_parent"                        android:layout_height="wrap_content"                        android:background="@null"                        android:completionThreshold="1"                        android:maxLength="20"                        android:singleLine="true"                        android:text=""                        android:textSize="14sp" />                    <TextView                        android:id="@+id/cityText"                        android:layout_width="fill_parent"                        android:layout_height="wrap_content"                        android:text="@string/citylabel"                        android:textColor="#FF7F24"                        android:textSize="16sp" />                    <com.join.LineAutoCompleteTextView                        android:id="@+id/cityEdit"                        android:layout_width="fill_parent"                        android:layout_height="wrap_content"                        android:background="@null"                        android:completionThreshold="1"                        android:maxLength="20"                        android:singleLine="true"                        android:text=""                        android:textSize="14sp" />                </TableRow>            </TableLayout>        </LinearLayout>        <TableLayout            android:layout_width="wrap_content"            android:layout_height="fill_parent"            android:layout_margin="05px"            android:orientation="vertical" >            <TableRow android:gravity="center_horizontal" >                <Button                    android:id="@+id/backButton"                    android:layout_width="wrap_content"                    android:layout_height="fill_parent"                    android:text="@string/backbutton_name"                    android:textSize="14sp" >                </Button>                <Button                    android:id="@+id/submitButton"                    android:layout_width="wrap_content"                    android:layout_height="fill_parent"                    android:text="@string/submitbutton_name"                    android:textSize="14sp" >                </Button>            </TableRow>        </TableLayout>    </TableLayout></ScrollView>